{	"manifest_version" : 2,
	"name" : "Multi Search Engine",
	"description" : "Type: Searches predefined search engines",
	"version" : "1.0",
	"applications" : {
		"gecko" : {
			"strict_min_version": "52.0a1"
		}
	},
	"omnibox": { "keyword" : "ms"},
	"background" : {
		"scripts": ["background.js"]
	},
	"permissions": ["tabs"],
	"icons" : { "48" : "icons/icon48.png", 
		"96" : "icons/icon96.png" }	
}